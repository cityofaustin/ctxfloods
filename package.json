{
  "name": "serverless-postgraphql",
  "version": "0.0.1",
  "description": "postgraphql test on serverless",
  "main": "handler.js",
  "config": {
    "PGCON": "postgresql://example:serverless@spny9q346s4odo.cg5ybcpnkj4u.us-east-1.rds.amazonaws.com:5432/floods"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "populate-schema": "psql $npm_package_config_PGCON -f ./populateDB/schema.sql",
    "populate-data": "psql $npm_package_config_PGCON -f ./populateDB/data.sql",
    "drop-data": "psql $npm_package_config_PGCON -f ./populateDB/drop.sql",
    "build-schema": "node ./pgCatalog/buildPgCatalog.js $npm_package_config_PGCON floods",
    "postgraphql-local": "postgraphql --connection $npm_package_config_PGCON --schema floods --default-role floods_anonymous --secret keyboard_kitten --token floods.jwt_token"
  },
  "author": "Richard",
  "license": "MIT",
  "dependencies": {
    "graphql": "^0.8.2",
    "pg-minify": "^0.4.2",
    "postgraphql": "^2.5.0"
  }
}
